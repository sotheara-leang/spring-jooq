<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<context:property-placeholder location="classpath:conf/application.properties"/>

	<context:component-scan base-package="com.example.springjooq"/>

	<import resource="logback/context-logback.xml"/>
	<import resource="context-datasource.xml"/>
	<import resource="context-jooq.xml"/>
	
</beans>
